Public Class Environment

Public Sub PromoteEquitableAccess()

'Declare variables
Dim envIneq As Boolean
Dim healthEnvironments As Integer

'Assign values
envIneq = True
healthEnvironments = 0

'Check if there is environmental inequality
If envIneq = True Then
	
	'Promote equitable access to healthy environments
	While healthEnvironments < 2000
		'Promote access
		healthEnvironments = healthEnvironments + 1
	Wend

Else
	'Do nothing
End If

End Sub

End Class